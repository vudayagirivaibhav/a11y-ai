{
  "name": "@a11y-ai/cli",
  "version": "0.0.0",
  "description": "CLI for a11y-ai audits.",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "a11y-ai": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "a11y-ai": "workspace:*"
  },
  "optionalDependencies": {
    "commander": "^12.1.0",
    "ora": "^8.0.1"
  }
}

